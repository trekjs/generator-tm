{
  "name": "generator-tm",
  "version": "0.1.0",
  "description": "Scaffold out a Trek.js module",
  "repository": "trekjs/generator-tm",
  "license": "MIT",
  "author": {
    "name": "Fangdun Cai",
    "email": "cfddream@gmail.com",
    "url": "fundon.me"
  },
  "keywords": [
    "yeoman-generator",
    "plugin",
    "boilerplate",
    "template",
    "scaffold",
    "trek",
    "module",
    "node_module",
    "trek-module",
    "init"
  ],
  "files": [
    "app"
  ],
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "xo && nyc ava"
  },
  "dependencies": {
    "humanize-url": "^1.0.1",
    "normalize-url": "^1.2.0",
    "superb": "^1.1.3",
    "underscore.string": "^3.0.3",
    "yeoman-generator": "^0.24.1"
  },
  "devDependencies": {
    "ava": "0.16",
    "pify": "2",
    "xo": "0.17",
    "yeoman-assert": "2",
    "yeoman-test": "1"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "es6"
    ],
    "semicolon": false,
    "space": true,
    "ignores": [
      "app/templates/**"
    ],
    "rules": {
      "curly": 0,
      "import/no-dynamic-require": 0,
      "import/no-named-as-default": 0,
      "max-lines": 0,
      "no-warning-comments": [
        "error",
        {
          "terms": [
            "todo",
            "fixme"
          ],
          "location": "anywhere"
        }
      ],
      "object-curly-spacing": [
        "error",
        "always",
        {
          "arraysInObjects": true,
          "objectsInObjects": true
        }
      ],
      "space-before-function-paren": [
        2,
        "always"
      ],
      "yoda": 0
    }
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
